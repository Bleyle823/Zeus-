{
    "name": "@elizaos/plugin-1inch",
    "version": "0.25.6-alpha.1",
    "description": "1inch cross-chain swap plugin for Eliza",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "test:coverage": "vitest run --coverage",
        "lint": "biome lint .",
        "lint:fix": "biome check --apply .",
        "format": "biome format .",
        "format:fix": "biome format --write ."
    },
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "@1inch/cross-chain-sdk": "^1.0.0",
        "web3": "^4.0.0",
        "zod": "^3.22.0",
        "tsup": "8.3.5"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "^20.0.0",
        "@vitest/coverage-v8": "^1.0.0",
        "typescript": "^5.0.0",
        "vitest": "^3.0.0"
    },
    "peerDependencies": {
        "@elizaos/core": "*"
    },
    "keywords": [
        "eliza",
        "plugin",
        "1inch",
        "cross-chain",
        "swap",
        "defi",
        "blockchain"
    ],
    "author": "ElizaOS",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/elizaos/eliza.git",
        "directory": "packages/plugin-1inch"
    },
    "bugs": {
        "url": "https://github.com/elizaos/eliza/issues"
    },
    "homepage": "https://github.com/elizaos/eliza#readme",
    "publishConfig": {
        "access": "public"
    }
}
